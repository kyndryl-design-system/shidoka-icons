{
  "name": "@kyndryl-design-system/shidoka-icons",
  "version": "0.0.0-semantic-release",
  "description": "Shidoka Icons",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyndryl-design-system/shidoka-icons.git"
  },
  "scripts": {
    "gulp": "gulp",
    "svgo:monochrome": "svgo --config ./svgo.monochrome.config.js -f ./dist/svg/monochrome -r -o ./dist/svg/monochrome",
    "svgo:duotone": "svgo --config ./svgo.duotone.config.js -f ./dist/svg/duotone -r -o ./dist/svg/duotone",
    "build": "npm run gulp && npm run svgo:monochrome && npm run svgo:duotone",
    "help": "svgo --help",
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@kyndryl-design-system/shidoka-applications": "^1.27.12",
    "@kyndryl-design-system/shidoka-foundation": "^1.7.1",
    "@storybook/addon-essentials": "^8.2.5",
    "@storybook/addon-links": "^8.2.5",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "^8.2.5",
    "@storybook/test": "^8.2.5",
    "@storybook/web-components": "^8.2.5",
    "@storybook/web-components-webpack5": "^8.2.5",
    "gulp": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "lit": "^3.1.4",
    "sass": "^1.77.8",
    "storybook": "^8.2.5",
    "storybook-preset-inline-svg": "^1.0.1",
    "svg-inline-loader": "^0.8.2",
    "svgo": "^3.3.2"
  }
}
