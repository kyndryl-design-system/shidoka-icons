{
  "name": "@kyndryl-design-system/shidoka-icons",
  "version": "0.0.0-semantic-release",
  "description": "Shidoka Icons",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyndryl-design-system/shidoka-icons.git"
  },
  "scripts": {
    "gulp": "gulp",
    "svgo:monochrome": "svgo --config ./svgo.monochrome.config.js -f ./dist/svg/monochrome -r -o ./dist/svg/monochrome",
    "svgo:duotone": "svgo --config ./svgo.duotone.config.js -f ./dist/svg/duotone -r -o ./dist/svg/duotone",
    "build": "npm run gulp && npm run svgo:monochrome && npm run svgo:duotone",
    "help": "svgo --help"
  },
  "devDependencies": {
    "gulp": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "svgo": "^3.3.2"
  }
}
